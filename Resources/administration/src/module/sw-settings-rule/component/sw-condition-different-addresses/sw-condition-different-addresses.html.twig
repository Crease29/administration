{% block sw_condition_fields %}
    {% block sw_condition_fields_value %}
        <sw-field type="select"
                  name="isDifferent"
                  v-model="isDifferent"
                  class="field field--select field--main">
            <template slot="options">
                <option v-for="item in selectValues" :value="item.value">
                    {{ $tc(item.label) }}
                </option>
            </template>
        </sw-field>
    {% endblock %}
{% endblock %}
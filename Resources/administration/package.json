  "private": true,
  "scripts": {
    "dev": "node build/dev-server.js",
    "build": "node build/build.js",
    "unit": "cross-env BABEL_ENV=test TESTING_ENV=single karma start test/unit/karma.conf.js --single-run",
    "unit-watch": "cross-env BABEL_ENV=test TESTING_ENV=watch karma start test/unit/karma.conf.js --auto-watch",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "security-check": "./node_modules/.bin/nsp check",
    "check-dependencies": "./node_modules/.bin/ncu",
    "generate-api-docs": "./node_modules/.bin/jsdoc src -r -c jsdoc.config.js",
    "storybook-watch": "start-storybook -p 9001 -s ./static -c .storybook",
    "storybook-generate": "build-storybook -s ./static -c .storybook -o ../../../../build/artifacts/storybook"
  },

<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service class="Shopware\Administration\Command\AdministrationDumpPluginsCommand" id="Shopware\Administration\Command\AdministrationDumpPluginsCommand">
            <argument type="service" id="kernel" />
            <tag name="console.command"/>
        </service>

        <service class="Shopware\Administration\Search\AuditLogSearch" id="Shopware\Administration\Search\AuditLogSearch">
            <argument id="service_container" type="service"/>
            <argument id="Shopware\Api\Entity\Search\Term\SearchTermInterpreter" type="service"/>
            <argument id="Shopware\Api\Entity\Search\Term\EntityScoreQueryBuilder" type="service"/>
            <argument id="Doctrine\DBAL\Connection" type="service"/>
        </service>

        <service class="Shopware\Administration\Controller\AdministrationController" id="Shopware\Administration\Controller\AdministrationController">
            <argument id="Shopware\Administration\Search\AuditLogSearch" type="service"/>
            <argument id="Shopware\Storefront\Twig\TemplateFinder" type="service"/>
        </service>
    </services>
</container>

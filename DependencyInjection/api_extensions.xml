<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service class="Shopware\Administration\Extension\AdministrationExtensionRegistry" id="shopware.administration.extension.registry">
            <argument type="collection"/>
            <argument id="shopware.extension.registry" type="service"/>
        </service>

        <service class="Shopware\Administration\Extension\ExtensionRegistrySubscriber" id="shopware.administration.extension.extension_registry_subscriber">
            <argument id="service_container" type="service"/>
            <tag name="kernel.event_subscriber"/>
        </service>

        <service class="Shopware\Administration\Command\AdministrationDumpPluginsCommand" id="shopware.administration.command.dump_plugins">
            <argument type="service" id="kernel" />
            <tag name="console.command"/>
        </service>
    </services>
</container>
